{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.RegularPages "Type" "in" .Site.Params.mainSections -}}
  {{- $.Scratch.Add "index" (dict "title" .Title "content" .Plain "permalink" .Permalink "summary" .Summary "date" (.Date.Format "2006-01-02") "tags" .Params.tags) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
